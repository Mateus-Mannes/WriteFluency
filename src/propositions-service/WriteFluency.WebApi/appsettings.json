{
  "ConnectionStrings": {
    "wf-postgresdb": "Host=localhost;Port=5432;Database=WriteFluencyDb;Username=postgres;Password=postgres"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedClients": "https://writefluency.com,http://localhost:4200,http://localhost:4000",
  "ExternalApis": {
    "OpenAI": {
      "BaseAddress": "https://api.openai.com/",
      "Routes": {
        "Completion": "v1/chat/completions",
        "Speech": "v1/audio/speech"
      },
      "Key": "{OPEN_AI_API_KEY}"
    },
    "TextToSpeech": {
      "Key": "{TEXT_TO_SPEECH_API_KEY}"
    },
    "News": {
      "BaseAddress": "https://api.thenewsapi.com/",
      "Routes": {
        "TopStories": "v1/news/top"
      },
      "Key": "{NEWS_API_KEY}"
    }
  },
  "Jwt": {
    "Issuer": "api.writefluency.com",
    "Audience": "writefluency.com",
    "Key": "{JWT-SECRET-KEY}"
  },
  "Authentication": {
    "Google": {
      "ClientId": "{GOOGLE_CLIENT_ID}",
      "ClientSecret": "{GOOGLE_CLIENT_SECRET}"
    }
  },
  "Propositions": {
    "DailyRequestsLimit": 20,
    "PropositionsLimitPerTopic": 30000,
    "NewsRequestLimit": 3
  },
  "RunNewsWorkerOnStartup": false,
  "FileStorage": {
    "Endpoint": "127.0.0.1:9000",
    "AccessKey": "admin",
    "SecretKey": "admin123"
  }
}