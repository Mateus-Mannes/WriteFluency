{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "AlloewdClients": "*",
  "ExternalApis":{
    "OpenAI": {
      "BaseAddress": "https://api.openai.com/",
        "Routes": {
          "Completion": "v1/chat/completions"
        },
      "Key": "{GPT_KEY}"
    },
    "GoogleCloud": {
      "Url": "https://texttospeech.googleapis.com/v1/text:synthesize",
      "Key": "{GC_KEY}"
    } 
  }
}
